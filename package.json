{
  "name": "kito",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "napi": {
    "name": "kito",
    "triples": {
      "additional": [
        "aarch64-apple-darwin",
        "aarch64-linux-android",
        "aarch64-unknown-linux-gnu",
        "aarch64-unknown-linux-musl",
        "aarch64-pc-windows-msvc",
        "armv7-unknown-linux-gnueabihf",
        "armv7-unknown-linux-musleabihf",
        "x86_64-unknown-linux-musl",
        "x86_64-unknown-freebsd",
        "i686-pc-windows-msvc",
        "armv7-linux-androideabi",
        "universal-apple-darwin",
        "riscv64gc-unknown-linux-gnu"
      ]
    }
  },
  "description": "A web framework written in Rust for TypeScript",
  "readme": "README.md",
  "repository": "https://github.com/nehu3n/kito",
  "homepage": "https://kito.pages.dev",
  "bugs": "https://github.com/nehu3n/kito/issues",
  "main": "index.js",
  "keywords": [],
  "author": "Nehu√©n <https://github.com/nehu3n>",
  "license": "MIT",
  "scripts": {
    "fmt:prettier": "prettier --config .prettierrc -w .",
    "bench": "node bench/start.js",
    "postinstall": "node scripts/postinstall.mjs",
    "artifacts": "napi artifacts",
    "core:build": "napi build --platform --release",
    "core:build:debug": "napi build --platform",
    "prepublishOnly": "napi prepublish -t npm",
    "universal": "napi universal",
    "version": "napi version"
  },
  "engines": {
    "node": ">= 10"
  },
  "devDependencies": {
    "@types/deno": "^2.2.0",
    "@types/node": "^22.13.9",
    "prettier": "3.4.2",
    "@napi-rs/cli": "^2.18.4"
  }
}
